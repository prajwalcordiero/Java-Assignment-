
import javax.swing.*;
import java.awt.*;
import java.awt.event.*;

public class Main   {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		JFrame f = new JFrame("java");
		 f.setLayout(null);
	      f.setSize(400, 400);
	      
	      JLabel l1 = new JLabel("LAB PROGGRAMS");
	      l1.setBounds(40, 80, 150, 50);
	      f.add(l1);
	      
	      JLabel l2 = new JLabel("ASSIGNMENT PROGRAMS");
	      l2.setBounds(190, 80, 170, 50);
	      f.add(l2);
	      
	      JLabel l3 = new JLabel("JAVA ASSIGNMENT");
	      l3.setBounds(130, 50, 150, 50);
	      f.add(l3);
	      
	      JButton b1 = new JButton("Electricity Bill");
	      b1.setBounds(15, 125, 150, 50);
	      f.add(b1);
	      
	      b1.addActionListener(new ActionListener() {
				
				public void actionPerformed(ActionEvent e) {
					Electricity_bill E = new Electricity_bill();
					
				}
	      });
	     

	      JButton b2 = new JButton("Calculator");
			b2.setBounds(190, 125, 150, 50);
	      f.add(b2);
	      
	      b2.addActionListener(new ActionListener() {
				
				public void actionPerformed(ActionEvent e) {
					Calci C = new Calci();
					
				}
	      });
	      

	      JButton b3 = new JButton("Binary to Decimal");
			b3.setBounds(190, 200, 150, 50);
	      f.add(b3);
	      
	      b3.addActionListener(new ActionListener() {
				
				public void actionPerformed(ActionEvent e) {
					Bin_to_Dec B = new Bin_to_Dec();
					
				}
	      });
	      
	      JButton b4 = new JButton("Employee Details");
			b4.setBounds(15, 200, 150, 50);
	      f.add(b4);
	      
	      b4.addActionListener(new ActionListener() {
				
				public void actionPerformed(ActionEvent e) {
					salaryslip S = new salaryslip();
					
				}
	      });
	      
	      f.setVisible(true);
	}

}
